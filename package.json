{
    "name": "stringifyit",
    "description": "Fast object stringifier",
    "version": "0.2.1",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "author": {
        "name": "Sergey Vasilenko",
        "email": "savstav@gmail.com",
        "url": "https://github.com/yarabey"
    },
    "license": "MIT",
    "keywords": [
        "stringify",
        "object",
        "es6"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/yarabey/stringifyit.git"
    },
    "homepage": "https://github.com/yarabey/stringifyit",
    "bugs": {
        "url": "https://github.com/yarabey/stringifyit/issues"
    },
    "scripts": {
        "test": "jasmine index.spec.js",
        "docs": "jsdoc2md index.js stringifier.js -t docs/readme-template.md > README.md",
        "bench": "node --expose-gc ./bench/speed.js",
        "clean": "rm -rf ./dist",
        "copy:package": "cp ./package.json ./dist/",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "release": "npm run clean && npm run build && npm run copy:package"
    },
    "engines": {
        "node": ">=12.6.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^26.0.14",
        "benchmark": "^2.1.3",
        "faker": "^3.1.0",
        "jest": "^26.4.2",
        "jsdoc-to-markdown": "^3.0.0",
        "ts-jest": "^26.4.0",
        "typescript": "^4.0.3"
    }
}
