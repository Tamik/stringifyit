{
    "name": "stringifyit",
    "description": "Fast object stringifier",
    "version": "0.2.1",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "author": {
        "name": "Sergey Vasilenko",
        "email": "savstav@gmail.com",
        "url": "https://github.com/yarabey"
    },
    "license": "MIT",
    "keywords": [
        "stringify",
        "object",
        "es6"
    ],
    "homepage": "https://github.com/yarabey/stringifyit",
    "bugs": {
        "url": "https://github.com/yarabey/stringifyit/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/yarabey/stringifyit.git"
    },
    "scripts": {
        "clean": "rm -rf ./dist ./coverage ./reports ./jest.results.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "copy:package": "cp ./package.json ./dist/",
        "release": "npm run clean && npm run build && npm run copy:package",
        "test:report": "jest --ci --preset=./configs/jest/ci.js",
        "test:watch": "jest --verbose --watch",
        "test:ci": "jest --ci --json",
        "test": "jest --verbose",
        "bench:speed": "node --expose-gc ./bench/speed.js",
        "bench": "npm run bench:speed"
    },
    "engines": {
        "node": ">=12.6.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^26.0.14",
        "benchmark": "^2.1.4",
        "faker": "^5.1.0",
        "jest": "^26.4.2",
        "jest-html-reporters": "^2.0.4",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.0",
        "typescript": "^4.0.3"
    }
}
